{
    "include": [
        "./src/**/*"
    ],
    "compilerOptions": {
        "jsx": "preserve",
        "target": "es5",
        "module": "esnext",
        "baseUrl": "./",
        "moduleResolution": "node",
        "paths": {
            "@/*": [
                "src/*"
            ]
        },
    },
    "vueCompilerOptions": {
        "experimentalShamefullySupportOptionsApi": true
    }
}
